source /usr/lib/wow/wowlib
export FSTYPE=""
echo "$FS_FILESYSTEMS" | while read dev dir fsid
do 
	echo $dev -- $dir
	if [ -n "${dev%%\#*}" ]; then # neither an empty line nor a comemnt line (# is first char)
		woweng $WOWRSRCEMODIFY GROUPID=${GRPID} \
			UUID=rsc_FS_${SAPSID}_${fsid} \
			DEVICE=$dev \
			MOUNTPOINT=$dir \
			FSTYPE=$FSTYPE \
			$wowxml/01-group_resource_fs.xml </dev/null
	fi
done

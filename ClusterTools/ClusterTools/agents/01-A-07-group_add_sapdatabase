DBTYPE=ORA
source /usr/lib/wow/wowlib
 case "$DBTYPE" in
	ORA ) xml=01-group_resource_appl_sapdatabase.xml
		;;
	* )   xml=01-group_resource_appl_sapdatabase_$DBTYPE.xml
		;;
 esac
 woweng $WOWRSRCEMODIFY UUID=rsc_SAPDatabase_$SAPSID \
		GROUPID=${GRPID} \
		SAPSID=$SAPSID  \
		DBTYPE=$DBTYPE \
		$wowxml/$xml

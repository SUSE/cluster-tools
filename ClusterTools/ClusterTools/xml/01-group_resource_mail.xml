<group id="$GROUPID">
 <primitive id="$UUID" class="ocf" type="MailTo" provider="heartbeat">
   <instance_attributes id="${UUID}_instance_attrs">
     <attributes>
       <nvpair id="${UUID}-attr1" name="email" value="$EMAILADDR"/>
       <nvpair id="${UUID}-attr2" name="subject" value="$EMAILSUBJ"/>
     </attributes>
   </instance_attributes>
 </primitive>
</group>
